{
  "name": "astridlindgren.se",
  "version": "0.8.4",
  "description": "Main app for astridlindgren.se",
  "main": "index.js",
  "scripts": {
    "test": "standard",
    "start": "node -r dotenv/config index.js",
    "rstart": "nodemon -e js,css -r dotenv/config index.js",
    "setup": "./bin/setup",
    "restart": "nodemon -r dotenv/config --inspect index.js -e js -i lib/components",
    "build:js:app": "browserify lib/app/index.js -t localenvify -t yo-yoify -t babelify -p common-shakeify -g unassertify -g uglifyify | uglifyjs -o public/index-$npm_package_version.js",
    "build:js": "npm run build:js:app",
    "build:css": "postcss lib/app/index.css --no-map -c postcss.config.js -o public/index-$npm_package_version.css",
    "build": "npm run build:css && npm run build:js"
  },
  "author": "http://www.itch.se",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/astridlindgrenco/astridlindgren.se.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "dotenv": "^4.0.0",
    "nodemon": "^1.18.3",
    "source-map-support": "^0.5.8",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "autoprefixer": "^8.6.5",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "common-shakeify": "^0.5.0",
    "dedent": "^0.7.0",
    "details-polyfill": "^1.1.0",
    "fun-component": "^4.1.0",
    "global-cache": "^1.2.1",
    "koa": "^2.4.1",
    "koa-body": "^2.5.0",
    "koa-cache-control": "^2.0.0",
    "koa-compose": "^4.0.0",
    "koa-compress": "^3.0.0",
    "koa-helmet": "^3.3.0",
    "koa-mount": "^3.0.0",
    "koa-no-trailing-slash": "^2.1.0",
    "koa-postcss-watch": "^1.0.5",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.3",
    "koa-static": "^4.0.3",
    "localenvify": "^1.0.1",
    "nanohtml": "^1.2.4",
    "normalize.css": "^8.0.0",
    "object-fit-images": "^3.2.3",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^4.1.1",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.2.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-import": "^10.0.0",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-selector-matches": "^3.0.1",
    "postcss-url": "^7.3.0",
    "prismic-dom": "^2.1.0",
    "prismic-element": "^1.0.0",
    "prismic-javascript": "^1.5.0",
    "prismic-richtext": "^0.4.2",
    "suitcss-components-grid": "^4.0.0",
    "suitcss-utils-flex": "^2.0.1",
    "suitcss-utils-size": "^2.0.1",
    "suitcss-utils-text": "^1.0.0",
    "sync-request": "^6.0.0",
    "uglify-js": "^3.4.7",
    "uglifyify": "^5.0.1",
    "uglifyjs": "^2.4.11",
    "unassertify": "^2.1.1",
    "watchify-middleware": "^1.8.0",
    "y18n": "^4.0.0",
    "yo-yoify": "^4.3.0"
  }
}
